{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "module": "NodeNext",
    "moduleResolution": "nodenext",
    "moduleSuffixes": [".ios", ".android", ".native", ".web", ""],
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts"]
}
